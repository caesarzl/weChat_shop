/**index.wxss**/
view {
  /* 使用主题颜色 */
  /* color: var(--themeColor) */
}
.index_swiper swiper {
  width: 750rpx;
  height: 340rpx;
}
.swiper_img {
  width: 100%;
}
.index_cate {
  display: flex;
}
.index_cate navigator {
  flex: 1;
  padding: 20rpx;
}
.cate_img {
  width: 100%;
}
.index_floor {

}
.floor_grop {

}
.floor_title {
  padding: 10rpx 0;
}
.floor_title image {
  width: 100%;
}
.floor_list {
  /* 清除浮动 */
  overflow: hidden;
}
.floor_list navigator {
  float: left;
  width: 33.33%;
}
/* 后4个超链接 */
.floor_list navigator:nth-last-child(-n+4) {
  /* 第一张图片的高度原图宽高： 232* 386 */
  /* 第一张图片 232 / 386 = 33.33vw / height */
  /* 第一张图片高度：height: 33.33vw * 386 / 232; */
  height: calc(33.33vw * 386 / 232 / 2);
  border-left: 10rpx solid #fff;
}
/* 2,3两个超链接 */
.floor_list navigator:nth-child(2),.floor_list navigator:nth-child(3) {
  border-bottom: 10rpx solid #fff;
}
.floor_list navigator image{
  width: 100%;
  height: 100%;
}